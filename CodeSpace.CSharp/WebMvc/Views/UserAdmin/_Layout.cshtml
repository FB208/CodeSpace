
@{
    ViewData["Title"] = "_Layout";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@RenderBody()
@{
    string alertMsg = TempData["AlertMsg"]+"";
    string href=TempData["Href"]+"";
    if (!string.IsNullOrWhiteSpace(alertMsg))
    {
        
        <script>
            alert("@Html.Raw(alertMsg)");
            document.location.href = '@href';
        </script>
    }

}

